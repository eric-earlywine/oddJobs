        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="job_details_tag"
                                 constraintName="fk_job_details_tag_tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>
        
    </changeSet>
</databaseChangeLog>
