<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity JobDetails.
    -->
    <changeSet id="20200313054100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="job_details_id"
                                 baseTableName="job_details_tag"
                                 constraintName="fk_job_details_tag_job_details_id"
                                 referencedColumnNames="id"
